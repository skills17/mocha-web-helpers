{
  "id": "single-test-filter",
  "serve": {
    "enabled": true,
    "mapping": {
      "/": "./",
      "/dist": "../../../dist",
      "/node_modules": "../../../node_modules"
    }
  },
  "groups": [
    {
      "match": "A.+",
      "displayName": "AandB"
    },
    {
      "match": "B.+"
    }
  ]
}
